ound\" name=\"bg\">\n            <input type=\"color\" title=\"Background Color:\" value=\"#ffffff\" selected>\n            <input type=\"image\" title=\"Background Image:\" value=\"@PROJECT_PATH@/assets/images/gadget-3.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color:\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"container-fluid\">\n        <!-- Title -->\n        <div class=\"title-block align-center\" mbr-if=\"showTitle || showSubtitle\">\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">CREATED NOW</h4>\n            <h3 class=\"mbr-section-title mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">Welcome to ShopAMP</h3>\n        </div>\n        <!-- Items -->\n        <div class=\"mbr-row mbr-jc-c\">\n            <!-- 1 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-4\">\n                <div class=\"wrapper-item\">\n                    <div class=\"item\">\n                        <div class=\"item__icon mbr-pb-3\" mbr-if=\"showIcon\" mbr-class=\"{'align-left': alignIcon == 1, 'align-center': alignIcon == 2, 'align-right': alignIcon == 3}\">\n                            <span class=\"iconfont-wrapper\">\n                                <span mbr-icon class=\"amp-iconfont mobi-mbri-mobile-2 mobi-mbri icon-1\"></span>\n                            </span>\n                        </div>\n                        <h5 class=\"item-title mbr-fonts-style mbr-medium mbr-pb-3\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" mbr-class=\"{'mbr-pb-4' : showCardText}\" data-app-selector=\".item-title, .mbr-section-btn\">Featured products\n                          </h5>\n                        <div mbr-buttons class=\"mbr-section-btn\" mbr-theme-style=\"display-7\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\" mbr-if=\"showButtons\">\n                        <a class=\"btn btn-info-outline\" href=\"http://mobirise.com\">\n                          Learn more\n                        </a>\n                        </div>\n                        \n                    </div>\n                </div>\n            </div>\n            <!-- 2 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-4\" mbr-if=\"amountCards > 1\">\n                <div class=\"wrapper-item\">\n                    <div class=\"item\">\n                        <div class=\"item__icon mbr-pb-3\" mbr-if=\"showIcon\" mbr-class=\"{'align-left': alignIcon == 1, 'align-center': alignIcon == 2, 'align-right': alignIcon == 3}\">\n                            <span class=\"iconfont-wrapper\">\n                                <span mbr-icon class=\"amp-iconfont mobi-mbri-plus mobi-mbri icon-2\"></span>\n                            </span>\n                        </div>\n                        <h5 class=\"item-title mbr-fonts-style mbr-medium mbr-pb-3\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" mbr-class=\"{'mbr-pb-4' : showCardText}\" data-app-selector=\".item-title, .mbr-section-btn\">Onsale Products</h5>\n                        <div mbr-buttons class=\"mbr-section-btn\" mbr-theme-style=\"display-7\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\" mbr-if=\"showButtons\">\n                          <a class=\"btn btn-info-outline\" href=\"http://mobirise.com\">\n                            Learn more\n                          </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!-- 3 -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-4\" mbr-if=\"amountCards > 2\">\n                <div class=\"wrapper-item\">\n                    <div class=\"item\">\n                        <div class=\"item__icon mbr-pb-3\" mbr-if=\"showIcon\" mbr-class=\"{'align-left': alignIcon == 1, 'align-center': alignIcon == 2, 'align-right': alignIcon == 3}\">\n                          <span class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont mobi-mbri-layers mobi-mbri icon-3\"></span>\n                          </span>\n                        </div>\n                        <h5 class=\"item-title mbr-fonts-style mbr-medium mbr-pb-3\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" mbr-class=\"{'mbr-pb-4' : showCardText}\" data-app-selector=\".item-title, .mbr-section-btn\">\n                        Top Rated Products</h5>\n                        <div mbr-buttons class=\"mbr-section-btn\" mbr-theme-style=\"display-7\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\" mbr-if=\"showButtons\">\n                          <a class=\"btn btn-info-outline\" href=\"http://mobirise.com\">\n                            Learn more\n                          </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
